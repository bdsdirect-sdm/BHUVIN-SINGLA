            "value": "John doe"
        },
        {
            "id": "fathername",
            "label": "Father's Name",
            "type": "text",
            "nameType": "fatherName",
            "required": true,
            "value": "Father"
        },
        {
            "id": "email",
            "label": "Email",
            "type": "email",
