            "value": "john.doe@example.com"
        },
        {
            "id": "phone",
            "label": "Phone",
            "type": "phone",
            "required": false,
            "value": "1234567890"
        },
        {
            "id": "country",
            "label": "Country",
            "type": "dropdown",
            "required": true,
            "options": [
                "USA",
                "Canada",
                "UK"
            ],
            "value": "USA"
        }
    ]
}